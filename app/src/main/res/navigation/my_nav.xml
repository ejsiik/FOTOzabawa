<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/fotoFragment">

    <fragment
        android:id="@+id/fotoFragment"
        android:name="com.example.fotozabawa.view.FotoFragment"
        android:label="fragment_foto"
        tools:layout="@layout/fragment_foto" >
        <action
            android:id="@+id/action_fotoFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.example.fotozabawa.view.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" >
        <action
            android:id="@+id/action_settingsFragment_to_fotoFragment"
            app:destination="@id/fotoFragment" />
    </fragment>
</navigation>